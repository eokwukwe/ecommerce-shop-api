{
  "name": "ecommerce-shop",
  "version": "1.0.0",
  "description": "e-commerce application",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec babel-node ./src/index.js",
    "pre-test-dev": "cross-env NODE_ENV=test babel-node ./src/test/migrate.js",
    "pretest": "babel-node ./src/test/migrate.js",
    "test": "jest --no-cache  --detectOpenHandles --runInBand --coverage --forceExit",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test:watch": "npm run pre-test-dev && jest --no-cache  --detectOpenHandles --runInBand --watchAll --coverage"
  },
  "keywords": [
    "ecommerce",
    "nodejs",
    "express",
    "caching"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@sendgrid/mail": "^6.3.1",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "express-winston": "^3.1.0",
    "fancy-log": "^1.3.3",
    "helmet": "^3.21.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "kue": "^0.11.6",
    "mailgen": "^2.0.8",
    "morgan": "^1.9.1",
    "mysql2": "^1.7.0",
    "nodemon": "^1.18.11",
    "redis": "^2.8.0",
    "sequelize": "^5.19.3",
    "sequelize-cli": "^5.4.0",
    "stripe": "^6.28.0",
    "uniqid": "^5.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.4.3",
    "babel-jest": "^24.7.1",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "mocha": "^6.1.4",
    "prettier": "^1.16.4",
    "supertest": "^4.0.2"
  }
}
